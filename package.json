{
  "name": "cypress_api_automation",
  "version": "1.0.0",
  "description": "API Automation Framework",
  "main": "index.js",
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json"
    }
  },
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:headless": "npx cypress run",
    "cy:report": "node cypress/cucumber-html-report.js"
  },
  "author": "Bilal Ahmed",
  "license": "ISC",
  "devDependencies": {
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^9.2.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3"
  }
}
